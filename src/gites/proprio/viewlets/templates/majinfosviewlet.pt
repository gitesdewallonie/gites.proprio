<div id="maj-info"
     tal:condition='viewlet/isVisible'
     tal:define="portal_url context/@@plone_portal_state/portal_url">
    <div id="titre-maj-info">
        Mise à jour des infos
    </div>
    <div id="titre-mes-donnees">
        Mes données
    </div>
    <div id="maj-info-menu">
        <a href=""
           tal:attributes="href string:${portal_url}/zone-membre/proprio-info">
           <b>Mes données personnelles</b>
        </a>
    </div>
    <div id="titre-hebergement">
        Hébergement
    </div>
    <div id="maj-info-menu"
         tal:repeat="gite viewlet/getGitesForProprio">
        <a href=""
           tal:attributes="href string:${portal_url}/zone-membre/hebergement-info?hebPk=${gite/token};
                           title gite/title">
           <b tal:replace="gite/title"/>
        </a>
    </div>
</div>
